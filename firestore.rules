rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  	match /users {
      allow read
      match /{user} {
          allow read: if request.auth.uid == request.resource.id
          allow write: if request.auth.uid == request.resource.id
      }
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
